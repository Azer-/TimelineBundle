<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="highco.timeline.notification_manager.class">Highco\TimelineBundle\Timeline\Notification\NotificationManager</parameter>
        <parameter key="highco.timeline.unread_notifications.class">Highco\TimelineBundle\Timeline\Notification\Notifier\UnreadNotificationNotifier</parameter>
    </parameters>

    <services>
        <service id="highco.timeline.notification_manager" class="%highco.timeline.notification_manager.class%">
        </service>

		<service id="highco.timeline.notifier.unread_notifications" class="%highco.timeline.unread_notifications.class%">
        </service>

    </services>

</container>
